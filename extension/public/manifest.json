{
  "short_name": "Imaginate",
  "name": "Imaginate - Create and Inspire",
  "version": "1.0",
  "author": "GJHF",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "http://127.0.0.1:5000/*"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/imaginate.png",
      "32": "/images/imaginate.png",
      "48": "/images/imaginate.png",
      "128": "/images/imaginate.png"
    }
  },
  "description": "Create and reshape beautiful images at the speed of thought.",
  "icons": {
    "16": "/images/imaginate.png",
    "32": "/images/imaginate.png",
    "48": "/images/imaginate.png",
    "128": "/images/imaginate.png"
  },
  "options_page": "modify.html",
  "options_ui": {
    "page": "modify.html"
  }
}
